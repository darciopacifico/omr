                    <source>1.6</source>
                    <target>1.6</target>
					<encoding>ISO8859-1</encoding>
		        </configuration>
			</plugin>


			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-eclipse-plugin</artifactId>
				<version>2.8</version>
