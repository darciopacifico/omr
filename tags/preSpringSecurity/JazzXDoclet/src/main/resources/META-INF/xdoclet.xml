<xdoclet-module>
    <!--
    br.com.jazz.jazzwizard.tagshandler.Wj2eeClassTagsHandler
    br.com.jazz.jazzwizard.tagshandler.Wj2eeMethodTagsHandler
    br.com.jazz.jazzwizard.subtask.Wj2eeSubTask
    -->

    <taghandler
        namespace="Wj2eeClass"
        class="br.com.jazz.jazzwizard.tagshandler.Wj2eeClassTagsHandler"
    />
    <taghandler
        namespace="Wj2eeMethod"
        class="br.com.jazz.jazzwizard.tagshandler.Wj2eeMethodTagsHandler"
    />

    <subtask
        name="wizardj2ee"
        implementation-class="br.com.jazz.jazzwizard.subtask.Wj2eeSubTask"
        parent-task-class="xdoclet.DocletTask"
    />
</xdoclet-module>
